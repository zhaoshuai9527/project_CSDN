
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>CSDN-用户注册</title>
    <script type="text/javascript" src="style/js/JavaScript.js"></script>
</head>
<body>
    <form id="register" action="" method="post">

        <input type="text" id="loginName" name="loginName" value="" tabindex="1" placeholder="请输入登录名" /><br/>
        <input type="password" id="password" name="password" tabindex="2" placeholder="请输入密码" /><br/>
        <input type="text" id="phone" name="phone" tabindex="3" placeholder="请输入电话"><br/>
        <input type="email" id="email" name="email" tabindex="4" placeholder="请输入邮箱地址"/><br/>
        <input type="submit" id="submitLogin" value="注 &nbsp; &nbsp; 册" />

    </form>

</body>
</html>
